{
  "presets": [
    "@vue/babel-preset-jsx",
    "@babel/env",
    [
      '@vue/app', {
        polyfills: [
          'es6.array.iterator',
          'es6.promise',
          'es7.promise.finally',
          'es6.symbol',
          'es6.array.find-index',
          'es7.array.includes',
          'es6.string.includes',
          'es6.array.find',
          'es6.object.assign'
        ]
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-jsx",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import",
  ]
}
